    "husky": "3.1.0",
    "jest": "^24.9.0",
    "library-utils": "3.0.2",
    "lint-staged": "8.1.1",
    "merge-stream": "1.0.1",
    "mkdirp": "0.5.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "prettier-eslint-cli": "^5.0.0",
    "react": "16.12.0",
    "react-docgen-typescript": "1.16.1",
    "react-dom": "16.12.0",
    "react-frame-component": "4.1.1",
    "react-styleguidist": "9.1.5",
    "region-flags": "1.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^12.0.1",
    "stylelint-value-no-unknown-custom-properties": "^2.0.0",
    "svgo": "1.0.5",
    "timezone-mock": "^1.0.0",
    "ts-jest": "24.0.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "uppercamelcase": "3.0.0",
    "vinyl-buffer": "1.0.1",
    "webpack": "4.28.0",
    "webpack-merge": "4.2.1"
  }
}
