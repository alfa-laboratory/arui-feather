    "gulp": "3.9.1",
    "gulp-gm": "0.0.8",
    "gulp-imagemin": "4.0.0",
    "gulp-rename": "1.2.2",
    "gulp.spritesmith": "6.7.0",
    "handlebars": "4.0.11",
